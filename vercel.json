{
  "version": 2,
  "rewrites": [
    {
      "source": "/play/:path*",
      "destination": "http://163.61.227.29:8000/play/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" }
      ]
    }
  ]
}